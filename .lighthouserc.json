{
  "ci": {
    "collect": {
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "Ready",
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/proposito"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:performance": [
          "error",
          {
            "minScore": 0.8
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}
